{
  "name": "shortcut-game",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "bun sync",
    "sync": "svelte-kit sync && lefthook install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "biome check . && bun check:svelte && tsc",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "fix": "biome check . --fix --unsafe",
    "fix:safe": "biome check . --fix"
  },
  "dependencies": {
    "pixi.js": "^8.8.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "@biomejs/biome": "^1.9.4",
    "lefthook": "^1.11.10",
    "typescript": "~5.7.3",
    "vite": "^6.2.0"
  }
}
